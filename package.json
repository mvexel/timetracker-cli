  "repository": {
    "type": "git",
    "url": "git+https://github.com/mvexel/timetracker-cli.git"
  },
  "bugs": {
    "url": "https://github.com/mvexel/timetracker-cli/issues"
  },
  "homepage": "https://github.com/mvexel/timetracker-cli#readme",
  "engines": {
    "node": ">=14.0.0"
  },
  "files": [
    "bin/",
    "lib/",
    "README.md",
    "LICENSE"
  ],
